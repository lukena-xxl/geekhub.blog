{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}
    <h1>{{ title }}</h1>
    <div class="mt-3 form-content">
        {{ form_start(form_add) }}
        <fieldset>{{ form_widget(form_add) }}</fieldset>
        {{ form_end(form_add) }}
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('ckeditor') }}
{% endblock %}
